source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'


target 'Chat2chat' do

  use_frameworks!

  # Pods for Chat2chat
  pod 'Firebase/Firestore'
  pod 'SnapKit', '~> 5.0.0'
  pod 'Firebase/Auth'
  pod 'RxSwift', '6.5.0'
  pod 'RxCocoa', '6.5.0'

  target 'Chat2chatTests' do
     inherit! :search_paths
     pod 'Firebase/Firestore'
     pod 'SnapKit', '~> 5.0.0'
     pod 'Firebase/Auth'
     pod 'RxSwift', '6.5.0'
     pod 'RxCocoa', '6.5.0'
  end

end
